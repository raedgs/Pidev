						<ul class="phone-wrap">
							<li>
								<span>Telephone Enquiry:</span>
								<a href="#">(+123) 123 321 345</a>
							</li>
						</ul>
					</div>
				</div>
				<!-- Header Top Left Area End Here -->
				<!-- Begin Header Top Right Area -->
				<div class="col-lg-9 col-md-8">
					<div class="header-top-right">
						<ul
							class="ht-menu">
							<!-- Begin User Area -->
							{% if is_granted('IS_AUTHENTICATED_FULLY') %}
								<li>
									<img class="user"
											{% if app.user.image is empty %}
												src="{{ asset('dist/img/avatar5.png')}}"
											{% else %}
												src="{{'/uploads/' ~ app.user.image }}"
											{% endif %}
										 alt="User profile picture">
									<a href="{{path('profile_front',{'id':app.user.id})}}" style="color:Red;font-family:Rubik;font-size:24px"> {{ app.user.username }}</a>
								</li>
							{% else %}
								<li class="fa fa-user" style='font-size:29px;color:black'>
									<a href="{{ path('login') }}" style="color:Red;font-family:Rubik;font-size:24px"> Login</a>
								</li>
							{% endif %}
							<!-- User Area End Here -->
						</ul>
					</div>
				</div>
				<!-- Header Top Right Area End Here -->
			</div>
		</div>
	</div>
	{% for flashSuccess in app.flashes('success') %}
		<div class="alert alert-success" role="alert">{{ flashSuccess }}</div>
	{% endfor %}
