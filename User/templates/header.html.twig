		<div class="alert alert-info" role="alert">{{ flashSuccess }}</div>
	{% endfor %}
	{% for flashSuccess in app.flashes('Email_Sent') %}
		<div class="alert alert-success" role="alert">{{ flashSuccess }}</div>
	{% endfor %}
